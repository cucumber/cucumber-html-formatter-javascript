    "mini-css-extract-plugin": "2.5.3",
    "mocha": "9.2.0",
    "puppeteer": "^13.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.49.0",
    "sass-loader": "12.4.0",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
